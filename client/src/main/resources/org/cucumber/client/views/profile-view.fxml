<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane maxHeight="420.0" prefHeight="420.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.cucumber.client.ProfileViewController">
   <children>
      <ToolBar prefHeight="41.0" prefWidth="600.0">
        <items>
          <Button mnemonicParsing="false" onAction="#onReturnButton" text="Retour" />
        </items>
      </ToolBar>
      <HBox alignment="CENTER" layoutX="51.0" layoutY="63.0" prefHeight="293.0" prefWidth="498.0" spacing="20.0">
         <children>
            <ImageView fx:id="avatar" fitHeight="150.0" fitWidth="150.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="https://img.myloview.com/murals/default-avatar-profile-in-trendy-style-for-social-media-user-icon-400-228654852.jpg" />
               </image></ImageView>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <VBox alignment="CENTER_LEFT" prefHeight="293.0" prefWidth="290.0" spacing="20.0">
                     <children>
                        <Label fx:id="username" text="Username" textFill="#6c0065">
                           <font>
                              <Font name="System Bold" size="16.0" />
                           </font>
                        </Label>
                        <HBox prefHeight="21.0" prefWidth="324.0" spacing="10.0">
                           <children>
                              <Label text="Age" />
                              <TextField fx:id="age" editable="false" prefHeight="25.0" prefWidth="50.0" />
                           </children>
                        </HBox>
                        <HBox layoutX="10.0" layoutY="48.0" prefHeight="85.0" prefWidth="256.0" spacing="10.0">
                           <children>
                              <Label prefHeight="18.0" prefWidth="20.0" text="Bio" />
                              <TextArea fx:id="description" editable="false" prefHeight="85.0" prefWidth="156.0" />
                           </children>
                        </HBox>
                        <Button fx:id="editButton" mnemonicParsing="false" onAction="#onEditButton" text="Modifier">
                           <font>
                              <Font size="10.0" />
                           </font>
                        </Button>
                     </children>
                  </VBox>
               </children>
            </HBox>
         </children>
      </HBox>
   </children>
</AnchorPane>
